{
    "test-plain": {
        "listen": "127.0.0.1:8000",
        "connect": "127.0.0.1:8100",
        "mode": "plain",
        "interceptors": [
            {
                "name": "hexdump"
            },
            {
                "name": "pcapdump",
                "args": {
                    "file": "dump.pcap",
                    "truncate": true
                }
            }
        ]
    },
    "test-tls": {
        "listen": "127.0.0.1:8001",
        "connect": "127.0.0.1:8101",
        "mode": "tls",
        "interceptors": [
            {
                "name": "hexdump"
            }
        ],
        "server": {
            "cert-pem": "cert.pem",
            "cert-key": "cert.key",
            "keylog": "key.log"
        },
        "client": {
            "server-name": "tlstap-test-server-name",
            "alpn": [
                "myprotoA", 
                "myprotoB"
            ],
            "keylog": "key.log"
        }
    },
    "test-detecttls": {
        "listen": "127.0.0.1:8000",
        "connect": "127.0.0.1:8100",
        "mode": "detecttls",
        "interceptors": [
            {
                "name": "hexdump"
            },
            {
                "name": "pcapdump",
                "args": {
                    "file": "dump.pcap",
                    "truncate": true
                }
            }
        ],
        "loglevel": "debug",
        "server": {
            "cert-pem": "test/certs/server.pem",
            "cert-key": "test/certs/server.key",
            "client-auth": "none"
        },
        "client": {
            "server-name": "server.local",
            "cert-pem": "test/certs/client-int-1-chain.pem",
            "cert-key": "test/certs/client-int-1.key",
            "tls-max": "1.1",
            "skip-verify": true,
            "ALPN": ["not h2"]
        }
    },
    "rot": {
        "listen": "127.0.0.1:8000",
        "connect": "127.0.0.1:8100",
        "mode": "detecttls",
        "interceptors": [
            {
                "name": "rot",
                "direction": "up",
                "args": {
                    "rot": 1,
                    "min-len": 2
                }
            }
        ],
        "server": {
            "cert-pem": "test/certs/server.pem",
            "cert-key": "test/certs/server.key"
        },
        "client": {
            "server-name": "server.local",
            "cert-pem": "test/certs/client-int-1-chain.pem",
            "cert-key": "test/certs/client-int-1.key",
            "tls-max": "1.1",
            "skip-verify": true
        }
    },
    "string-replacer": {
        "listen": "127.0.0.1:8000",
        "connect": "127.0.0.1:8100",
        "mode": "detecttls",
        "interceptors": [
            {
                "name": "replacer",
                "args": {
                    "replacements": {
                        "ping": "pong",
                        "hello": "byeee"
                    }
                }
            }
        ],
        "server": {
            "cert-pem": "test/certs/server.pem",
            "cert-key": "test/certs/server.key"
        },
        "client": {
            "server-name": "server.local",
            "cert-pem": "test/certs/client-int-1-chain.pem",
            "cert-key": "test/certs/client-int-1.key",
            "tls-max": "1.1",
            "skip-verify": true
        }
    },
    "string-replacer-py": {
        "listen": "127.0.0.1:8000",
        "connect": "127.0.0.1:8100",
        "mode": "detecttls",
        "interceptors": [
            {
                "name": "bridge",
                "args": {
                    "connect": "127.0.0.1:5555"
                }
            }
        ],
        "server": {
            "cert-pem": "test/certs/server.pem",
            "cert-key": "test/certs/server.key"
        },
        "client": {
            "server-name": "server.local",
            "cert-pem": "test/certs/client-int-1-chain.pem",
            "cert-key": "test/certs/client-int-1.key",
            "tls-max": "1.1",
            "skip-verify": true
        }
    }
}