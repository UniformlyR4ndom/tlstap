{
    "proxies":{
        "example-tls-basic": {
            "mode": "tls",
            "listen": "127.0.0.1:443",
            "connect": "74.125.29.101:443",
            "server": "basic-server",
            "client": "basic-client",
            "interceptors": ["hexdump"]
        },
        "example-tls-mux": {
            "mode": "tls-mux",
            "listen": "127.0.0.1:8443",
            "connect": "127.0.0.1:9443",
            "server": "basic-server",
            "client": "basic-client",
            "mux": {
                "test": {
                    "sni-matches": [".*test.*"],
                    "connect": "127.0.0.1:9000",
                    "tls-server": "basic-server",
                    "tls-client": "basic-client",
                    "interceptors": ["hexdump"]
                },
                "example": {
                    "sni-matches": ["*.example.*", ".*demo.*"],
                    "connect": "127.0.0.1:9001",
                    "tls-server": "basic-server",
                    "tls-client": "basic-client",
                    "interceptors": ["hexdump"]
                }
            }
        }
    },
    "tls-server-configs": {
        "basic-server": {
            "cert-pem": "test/certs/server.pem",
            "cert-key": "test/certs/server.key"
        }
    },
    "tls-client-configs": {
        "basic-client": {
            "skip-verify": true
        },
        "demo-client": {
            "alpn": ["test", "alpn is fun"],
            "server-name": "server.local",
            "cert-pem": "test/certs/client-int-1-chain.pem",
            "cert-key": "test/certs/client-int-1.key",
            "tls-max": "1.2",
            "tls-min": "1.1",
            "skip-verify": true,
            "keylog": "key.log"
        }
    },
    "interceptors": {
        "hexdump": {
            "name": "hexdump"
        },
        "pcapdump": {
            "name": "pcapdump",
            "args": {
                "file": "dump.pcap",
                "truncate": true
            }
        }
    }
}